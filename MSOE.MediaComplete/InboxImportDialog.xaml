<Window x:Class="MSOE.MediaComplete.InboxImportDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Inbox Import"
        Height="150"
        Width="300"
        ResizeMode="NoResize"
        ShowInTaskbar="False"
        WindowStartupLocation="CenterOwner">
    <Grid>
        <TextBlock Margin="10"
            Name="MessageTextBlock"
            Text="This is the test text." />
        <CheckBox Name="StopShowingCheckBox" Margin="10,50,0,0">Don't show this message again.</CheckBox>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" VerticalAlignment="Bottom">
            <Button Name="OkButton" Click="okButton_Click" IsDefault="True" MinWidth="65" Margin="0,0,0,10">Move Them</Button>
            <Button Name="CancelButton" Click="CancelButton_OnClick" IsCancel="True"  MinWidth="65" Margin="10,0,10,10">Not Yet</Button>
        </StackPanel>
    </Grid>
</Window>
