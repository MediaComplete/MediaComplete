<Window x:Class="MSOE.MediaComplete.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Media Complete" Height="459" Width="711">
    <Grid Margin="0,0,0,0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="208*"/>
            <ColumnDefinition Width="27*"/>
        </Grid.ColumnDefinitions>
        <DockPanel Grid.ColumnSpan="2">
            <DockPanel Height="Auto" HorizontalAlignment="Stretch">
                <TreeView  Width="75"/>
                <TreeView Width="75"/>
                <TreeView Width="75" />
            </DockPanel>
            <StackPanel Height="86">
                <Slider/>
                <StackPanel Height="64" Orientation="Horizontal" HorizontalAlignment="Center">
                    <Button Content="Previous" Width="75" Click="Button_Click"/>
                    <Button Content="Play" Width="75"/>
                    <Button Content="Skip" Width="75"/>
                </StackPanel>
            </StackPanel>
        </DockPanel>
    </Grid>
</Window>
