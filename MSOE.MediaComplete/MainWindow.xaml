<Window x:Class="MSOE.MediaComplete.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Media Complete" Height="350" Width="525">
    <Grid Margin="0,0,0,0">
        <StackPanel Height="320" Width="515">
            <StackPanel Height="234" Orientation="Horizontal">
                <TreeView Width="178"/>
                <TreeView Width="174"/>
                <TreeView Width="164"/>
            </StackPanel>
            <StackPanel Height="86">
                <Slider/>
                <StackPanel Height="64" Orientation="Horizontal" HorizontalAlignment="Center">
                    <Button Content="Previous" Width="75" Click="Button_Click"/>
                    <Button Content="Play" Width="75"/>
                    <Button Content="Skip" Width="75"/>
                </StackPanel>
            </StackPanel>
        </StackPanel>

    </Grid>
</Window>
